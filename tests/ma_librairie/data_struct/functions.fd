c_init_rectangle:
    help: Initialize a rectangle
    langage: C
    arguments:
    - name: r
      type: rectangle
      intent: in
      help: Input rectangle
    - name: width
      type: u4
      intent: in
      help: Width of the rectangle
    - name: height
      type: u4
      intent: in
      help: Height of the rectangle
for_area:
    help: Compute the area of a rectangle
    langage: F
    arguments:
    - name: r
      type: rectangle
      intent: in
      help: Input rectangle
    - name: area
      type: u4
      intent: out
      help: Area of the rectangle
